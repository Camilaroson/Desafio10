 <h1>Lista de productos</h1>
 
 <table class="table table-striped table-dark">
        <thead>
                <tr>
                
                        <th scope="col">Titulo</th>
                        <th scope="col">Precio</th>
                        <th scope="col">Imagen</th>
                </tr>
        </thead>
         <tbody>
        {{#each productos}}
                <tr>
                        
                        <td>{{this.titulo}}</td>
                        <td>{{this.precio}}</td>
                        <td> <img src="{{this.imagen}}"></td>
                </tr>
        {{/each}}
        </tbody>
 </table>